<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="InariPlaysGenNHibernate"
		                   namespace="InariPlaysGenNHibernate.EN.Prueba">
			<class name="VideojuegoEN" table="[Videojuego]">
				  	
						<id name="VideojuegoId" column="[videojuegoIdVideojuego]" type="integer">
							<generator class="identity"/>
						</id>
					<property name="Nombre" type="string"> 
						<column name="[nombre]"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="[descripcion]"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<bag name="Producto" >
							<key>
								<column name="[FK_videojuegoIdVideojuego_idVideojuego]"/>
					      	</key>
							<one-to-many class="InariPlaysGenNHibernate.EN.Prueba.ProductoEN"/>
					    </bag>
					
					
					
					
					
		
		  </class>
		  
		  <query name="VideojuegoENReadAllHQL">from VideojuegoEN</query>
		</hibernate-mapping>
	